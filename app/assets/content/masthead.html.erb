<header id="malmo-masthead">
  <div class="nav-logo"><a href="<%= APP_CONFIG['dashboard_base_url'] %>">Komin</a></div>

  <ul id="nav-triggers">
    <li id="nav-search-trigger"><a href="<%= APP_CONFIG['site_search_url'] %>">Sök <span class="icon-search icon-large"></span></a></li>
    <li id="nav-menu-trigger"><a href="#">Meny <span class="icon-reorder icon-large"></span></a></li>
  </ul>

  <nav class="masthead-main">
    <ul>
      <li id="nav-dashboard"><a href="<%= APP_CONFIG['dashboard_base_url'] %>">Min sida</a></li>
      <li id="nav-my-department">
        <a href="<%= APP_CONFIG['dashboard_base_url'] %>my_roles">Min förvaltning</a>
      </li>
      <li id="nav-my-workingfield">
        <a href="<%= APP_CONFIG['dashboard_base_url'] %>my_roles">Mitt arbetsfält</a>
      </li>
      <li id="nav-our-organization"><a href="<%= APP_CONFIG['sv_base_url'] %>Var-kommun">Vår kommun</a></li>
      <li id="nav-hrutan"><a href="<%= APP_CONFIG['sv_base_url'] %>minanstallning">Min anställning</a></li>
      <li id="nav-more" class="dropdown">
        <a class="dropdown-toggle more-items" data-toggle="dropdown" href="#">Mer <span class="icon-caret-down icon-large"></span></a>
        <ul class="dropdown-menu">
          <li><a href="<%= APP_CONFIG['dashboard_base_url'] %>my_profile">Min profil</a></li>
          <li><a href="<%= APP_CONFIG['sv_base_url'] %>blogg">Blogg</a></li>
          <li><a href="<%= APP_CONFIG['sv_base_url'] %>forum">Forum</a></li>
          <li><a href="<%= APP_CONFIG['sv_base_url'] %>nyheter">Nyheter</a></li>
          <li><a href="<%= APP_CONFIG['sv_base_url'] %>wiki">Wiki</a></li>
          <li><a href="<%= APP_CONFIG['sv_base_url'] %>webbvideo">Webbvideo</a></li>
        </ul>
      </li>
    </ul>
  </nav>

  <nav id="masthead-others">
    <ul>
      <li class="map">
        <a href="<%= APP_CONFIG['sv_base_url'] %>karta" title="Karta" alt="Karta"><span class="icon-map-marker icon-large"></span></a>
      </li>
      <li class="phone">
        <a href="http://srvwinucweb01.uc.malmo.se/cmgoffice" title="Styr telefonen" alt="Styr telefonen"><span class="icon-phone icon-large"></span></a>
      </li>
    </ul>
  </nav>

  <div id="masthead-search" class="malmo-form">
    <form action="<%= APP_CONFIG['site_search_url'] %>" method="get" id="masthead-search-intranet">
      <div class="input-append">
        <input name="q" class="q" placeholder="Ett eller flera sökord" type="text" data-autocomplete-url="<%= APP_CONFIG['site_search_url'] %>/autocomplete" />
        <input class="btn btn-primary" name="search-intranet" type="submit" value="Sök på Komin"/>
      </div>
    </form>

    <form action="<%= APP_CONFIG['dashboard_base_url'] %>users" id="masthead-search-person" method="get">
      <div class="input-append">
        <input name="term" class="text" placeholder="För- och/eller efternamn" type="text"/>
        <input class="btn btn-primary" type="submit" value="Sök person"/>
      </div>
    </form>
  </div>
</header>
