<header id="malmo-masthead">
  <div class="nav-logo">
    <a href="<%= APP_CONFIG['sv_base_url']%>"></a>
  </div>

  <ul id="nav-triggers">
    <li id="nav-menu-trigger"><a href="#">Meny <span class="icon-reorder icon-large"></span></a></li>
    <li id="nav-search-trigger"><a href="<%= APP_CONFIG['site_search_url'] %>">Sök <span class="icon-search icon-large"></span></a></li>
  </ul>

  <nav id="masthead-main">
    <ul>
      <li class="nav-citizen">
        <a href="<%= APP_CONFIG['sv_base_url']%>">Medborgare</a>
      </li>
      <li class="nav-business">
        <a href="<%= APP_CONFIG['sv_base_url']%>Foretagare.html">Företagare</a>
      </li>
      <li class="nav-turist">
        <a href="<%= APP_CONFIG['sv_base_url']%>Turist.html">Turist</a>
      </li>
      <li class="nav-politics">
        <a href="<%= APP_CONFIG['sv_base_url']%>Kommun--politik.html">Kommun &amp; politik</a>
      </li>
      <li class="nav-services">
        <a href="<%= APP_CONFIG['sv_base_url']%>Blanketter--tjanster.html">Blanketter &amp; tjänster</a>
      </li>
    </ul>
  </nav>

  <nav id="masthead-others">
    <ul>
      <li>
        <a href="<%= APP_CONFIG['sv_base_url']%>lattlast">Lättläst</a>
      </li>
      <li>
        <a href="<%= APP_CONFIG['sv_base_url']%>a-o">A–Ö</a>
